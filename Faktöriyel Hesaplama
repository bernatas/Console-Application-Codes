class Program
	{
		public static void Main(string[] args)
		{
			int sayi,sonuc=1;
			Console.Write("Sayıyı giriniz=");
			sayi=Convert.ToInt32(Console.ReadLine());
			
			for(int i=1;i<=sayi;i++)
			{
				sonuc=sonuc*i;
				
			}
			
			Console.WriteLine(sonuc);
			Console.ReadLine();
				
            
        }
			
		
		}
